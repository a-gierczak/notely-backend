{
  "name": "notely-backend",
  "version": "0.0.0",
  "description": "description",
  "author": "Aleksander Gierczak",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "ts-node --transpile-only -r tsconfig-paths/register src/main.ts",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nest-middlewares/morgan": "^5.1.1",
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/jwt": "^0.2.0",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/typeorm": "^5.2.2",
    "@types/bcrypt": "^3.0.0",
    "@types/date-fns": "^2.6.0",
    "@types/lodash": "^4.14.118",
    "@types/mime-types": "^2.1.0",
    "@types/passport-jwt": "^3.0.1",
    "@types/ramda": "types/npm-ramda#dist",
    "aws-sdk": "^2.359.0",
    "bcrypt": "^3.0.2",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.x",
    "csprng": "^0.1.2",
    "date-fns": "^1.29.0",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.21",
    "mysql": "^2.16.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "ramda": "^0.25.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "typeorm": "^0.2.8",
    "typescript": "3.2.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/csprng": "^0.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/multer": "^1.3.7",
    "@types/node": "^10.7.1",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
